{
  "name": "german-tax-sim",
  "version": "1.0.0",
  "description": "Interactive simulator for German income tax policy (ยง32a EStG) based on Destatis 2021 statistics",
  "scripts": {
    "build": "esbuild main.jsx --bundle --jsx=automatic --outfile=public/app.js",
    "build:min": "esbuild main.jsx --bundle --jsx=automatic --minify --outfile=public/app.js",
    "dev": "esbuild main.jsx --bundle --jsx=automatic --outfile=public/app.js --watch",
    "serve": "npx serve public"
  },
  "keywords": ["tax", "germany", "simulator", "einkommensteuer", "react"],
  "author": "ihsoyCT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihsoyCT/german-tax-sim.git"
  },
  "homepage": "https://github.com/ihsoyCT/german-tax-sim#readme",
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.3"
  }
}
